{
    "name": "background",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.11.9",
        "@testing-library/react": "^11.2.5",
        "@testing-library/user-event": "^12.8.1",
        "@types/jest": "^26.0.20",
        "@types/node": "^12.20.4",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-scripts": "4.0.3",
        "typescript": "^4.2.3",
        "web-vitals": "^1.1.0"
    },
    "devDependencies": {
        "awesome-typescript-loader": "^5.2.1",
        "babel-eslint": "^10.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "cra-build-watch": "^3.4.0",
        "eslint": "^7.21.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "prettier": "^2.2.1",
        "webpack-cli": "^4.5.0"
    },
    "scripts": {
        "lint": "npx eslint ./src --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "npx eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
        "watch": "concurrently \"npm run watch:react\" \"npm run watch:bundle\"",
        "watch:react": "cra-build-watch -b ../build",
        "watch:bundle": "webpack --mode production --watch"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
