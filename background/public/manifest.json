{
  "name": "Uber eats stats",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "/popup/index.html"
  },
  "content_security_policy": "script-src 'self'",
  "background": {
    "scripts": ["hot-reload.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://www.ubereats.com/*"],
      "js": ["/static/js/main.js"]
    }
  ]
}
